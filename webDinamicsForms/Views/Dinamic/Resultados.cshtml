
@{
    ViewBag.Title = "Resultados";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div id="vTbl">
    <div class="row">
        <div class="col-lg-12">
            <div class="card-box">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-3">
                        <label for="cbxMunicipio">Municipio</label>
                        <select class="form-control" id="cbxMunicipio"></select>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-3">
                        <label for="cbxSeccion">Seccion</label>
                        <select class="form-control" id="cbxSeccion"></select>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-3">
                        <label for="cbxSubSeccion">Sub Seccion</label>
                        <select class="form-control" id="cbxSubSeccion"></select>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12">
                        <div class="card-box table-responsive">
                            <table id="tblJson" class="table table-striped table-bordered dt-responsive nowrap">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th>Id</th>
                                        <th>Nombre</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="vAdd">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h3 id="lblTitulo"></h3>
                    <br />
                </div>
                <div class="card-body">
                    <div style="overflow-x:auto">
                        <div id="tblDinamic" class="datagrid">

                        </div>
                    </div>
                    <button type="button" class="btn btn-info" id="btnReturn">Regresar</button>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Content/script/Resultados.js"></script>



